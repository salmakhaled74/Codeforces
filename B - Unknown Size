#include <bits/stdc++.h>
#include<iostream>
using namespace std;

int main()
{
    long long int n,cnt=0;
    cin >> n;
    for(int i=1; i<=n; i++)
    {
        if(n % i == 0)
        {
            cnt++;
        }
    }
    cout << cnt << endl;
    for(int i=1; i<=n; i++)
    {
        if(n % i == 0)
        {
            cout << i << " ";
        }
    }
    return 0;
}
